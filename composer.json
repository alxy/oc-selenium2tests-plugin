{
    "name": "Initbiz/Selenium2tests",
    "type": "october-plugin",
    "description": "Add functionality to write tests in Selenium 2",
    "homepage": "http://init.biz",
    "keywords": ["october", "octobercms", "php", "selenium", "selenium 2", "testing"],
    "license": "BSD 3-clause",
    "authors": [
        {
            "name": "Tomasz Strojny",
            "email": "tomasz@init.biz"
        }
    ],
    "require-dev": {
        "modelizer/selenium": "~1.0"
    },
    "autoload": {
        "classmap": [
            "./classes"
        ],
        "files": [
            "./traits/CustomDataProviders.php",
            "./traits/OctoberSeleniumHelpers.php",
            "./traits/SeleniumHelpers.php"
        ]
    },
    "require" : {
        "phpunit/phpunit": ">4.0",
        "phpunit/phpunit-selenium": ">1.2"
    }
}
